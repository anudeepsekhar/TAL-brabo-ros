<launch>
	<arg name="brabo" />
	<param name="robot_description" command="$(find xacro)/xacro --inorder $(find brabo_description)/urdf/brabo_gripper.xacro" />
	<param name="use_gui" value="true"/>
	<param name="ignore_timestamp" value="true" />
	<param name="rate" value="100" />
	<node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />
	<node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" />
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find brabo_description)/rviz/urdf.rviz" required="true" />
</launch>
